{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"dev": {},
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"./.next/**",
				"./dist/**"
			]
		},
		"test": {},
		"lint": {
			"outputs": []
		},
		"deploy": {
			"dependsOn": [
				"build",
				"test",
				"lint"
			],
			"outputs": []
		}
	}
}